  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tchaguitos/correios-services/issues"
  },
  "homepage": "https://github.com/tchaguitos/correios-services#readme",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "consign": "^0.1.6",
    "cors": "^2.8.4",
    "express": "^4.16.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
